{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "./server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./studio/index.html",
      "use": "@vercel/sanity"
    },
    {
      "src": "./package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/api/login",
      "destination": "/server/index.js"
    },
    {
      "source": "/api/sign_up",
      "destination": "/server/index.js"
    },
    {
      "source": "/studio/(.*)",
      "destination": "/studio/index.html"
    }
  ]
}